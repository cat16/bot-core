{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 2],
        "object-literal-sort-keys": [true, "match-declaration-order"],
        "interface-name": [true, "never-prefix"],
        "array-type": [true, "array"]
    },
    "rulesDirectory": []
}